<base robot_number="0">
	<block class="timer" label="oneMin" name="repeat for 1 minute" color="1291754">    
		<bracket/>
		<file>timing/oneMin.blk</file>
		<blocksIn>
			<block class="longhorn" label="verHead" name="%d head" color="14584890">    
				<dropdown>        
					<dds value="500">RAISE</dds>
					<dds value="950">LOWER</dds>
					<selected>RAISE</selected>
				</dropdown>
				<file>actions/verHead.blk</file>
			</block>
			<block class="longhorn" label="verHead" name="%d head" color="14584890">    
