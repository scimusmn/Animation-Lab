				</dropdown>
				<file>actions/horHead.blk</file>
			</block>
			<block class="longhorn" label="horHead" name="move head %d" color="14584890">    
				<dropdown>        
					<dds value="950">LEFT</dds>
					<dds value="775">CENTER</dds>
					<dds value="600">RIGHT</dds>
					<selected>CENTER</selected>
				</dropdown>
				<file>actions/horHead.blk</file>
			</block>
		</blocksIn>
	</block>
</base>